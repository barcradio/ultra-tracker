{
  "name": "ultra-tracker",
  "version": "1.0.0-beta.2",
  "description": "Track athlete times in ultra marathons.",
  "main": "./out/main/index.js",
  "author": {
    "name": "Bridgerland Amatuer Radio Club",
    "url": "https://barconline.org"
  },
  "homepage": "https://github.com/barcradio/ultra-tracker",
  "appId": "barcradio.ultra-tracker",
  "license": "MIT",
  "repository": {
    "repository": {
      "type": "git",
      "url": "git+https://github.com/barcradio/ultra-tracker.git"
    }
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:watch": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "prepare": "husky"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-router": "^1.48.1",
    "@tanstack/react-virtual": "^3.10.2",
    "@tw-classed/react": "^1.7.0",
    "@uidotdev/usehooks": "^2.4.1",
    "ajv": "8.0.0",
    "better-sqlite3": "^11.1.2",
    "class-variance-authority": "^0.7.0",
    "csv-parse": "^5.5.6",
    "date-fns": "^3.6.0",
    "electron-log": "^5.2.0",
    "electron-store": "8.2.0",
    "electron-updater": "^6.2.1",
    "focus-trap-react": "^10.2.3",
    "object-hash": "^3.0.0",
    "primereact": "^10.8.2",
    "react-hook-form": "^7.52.1",
    "react-markdown": "^9.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-toc": "^9.0.0",
    "tailwind-merge": "^2.3.0",
    "typeorm": "^0.3.20",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/format": "^19.3.0",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@tanstack/router-devtools": "^1.48.1",
    "@tanstack/router-vite-plugin": "^1.47.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^18.19.44",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.4",
    "is-core-module": "^2.15.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "tw-colors": "^3.3.1",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0",
    "vite": "^5.4.0",
    "vite-plugin-svgr": "^4.2.0"
  }
}